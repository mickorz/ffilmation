// Noise

package org.ffilmation.engine.datatypes {
	

		/**
		* This object stores a tridimensional point
		*/
		public class fPoint3d {
		
			/** X coordinate */
			public var x:Number

			/** Scene Y coordinate */
			public var y:Number

			/** Scene Z coordinate */
			public var z:Number
			
			/**
			* Constructor for the fPoint3d class
			*/
			function fPoint3d(x:Number,y:Number,z:Number):void {
				 this.x = x
				 this.y = y
				 this.z = z
			}
			

			
		}
}

